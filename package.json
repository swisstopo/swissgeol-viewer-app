{
  "name": "ngm",
  "private": true,
  "scripts": {
    "lint": "eslint src/ test/",
    "extract-from-assets": "node -r esm scripts/get_assets_properties.js > src/assets_properties.js",
    "extract-i18n": "i18next",
    "start": "npm run webpack-start",
    "webpack-start": "node scripts/environment.js && node scripts/generateManuals.js && webpack serve",
    "rollup-start": "node scripts/environment.js && node scripts/generateManuals.js && rollup --environment SERVE:true -c --watch --no-treeshake",
    "build": "node scripts/environment.js $RELEASE_NAME && node scripts/generateManuals.js && rm -rf dist/* && rollup --environment mode:production -c && scripts/create_versions.sh",
    "test": "mocha --exit"
  },
  "dependencies": {
