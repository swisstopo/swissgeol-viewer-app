    "@aws-sdk/s3-request-presigner": "^3.3.0",
    "@geoblocks/cesium-compass": "0.3.0",
    "@geoblocks/cesium-view-cube": "0.0.1",
    "@geoblocks/ga-search": "^0.0.18",
    "@sentry/browser": "6.0.1",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "cesium": "1.77.0",
    "event-target-shim": "^5.0.1",
    "fomantic-ui-css": "^2.8.7",
    "i18next": "19.8.4",
    "i18next-xhr-backend": "^3.2.2",
    "interactjs": "^1.10.2",
    "jquery": "^3.5.1",
    "jszip": "^3.5.0",
    "lit-element": "^2.4.0",
    "lit-html": "^1.3.0",
    "loc-i18next": "^0.1.4",
    "proj4": "2.7.0",
    "typeface-source-sans-pro": "1.1.13"
  },
