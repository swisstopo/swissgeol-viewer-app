{
  "name": "ngm",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint src/ --ext .ts,.js",
    "extract-from-assets": "node scripts/get_assets_properties.js src/assets_properties.js",
    "extract-i18n": "i18next -c i18next-parser.config.cjs",
    "start": "npm run webpack-start",
    "build-static": "node scripts/environment.js $RELEASE_NAME && node scripts/generateManuals.js && bash scripts/legal_md_to_html.sh",
    "webpack-start": "npm run build-static && webpack serve",
    "rollup-start": "npm run build-static && rollup --environment SERVE:true -c --watch --no-treeshake",
    "build": "rm -rf dist/* && npm run build-static && rollup --environment mode:production -c && bash scripts/create_versions.sh; find dist -type l -exec rm {} +",
    "check": "npm run check-main && npm run check-ut",
    "check-main": "tsc src/index.ts --noEmit --allowJs --skipLibCheck --target es2018 --moduleResolution node --experimentalDecorators",
    "check-ut": "tsc src/test/*.ts src/test/*.js  --noEmit --allowJs --skipLibCheck --esModuleInterop true --target es2018 --moduleResolution node --experimentalDecorators",
    "ut": "mocha --exit --experimental-specifier-resolution=node -r jsdom-global/register -r src/test/jquery.register.js",
    "test": "npm run ut && npm run check",
    "cypress:open": "cypress open",
    "cypress:run": "./scripts/cypress_run",
    "test:e2e": "start-server-and-test start http-get://localhost:8000 cypress:run"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "3.252.0",
    "@aws-sdk/client-s3": "3.252.0",
    "@aws-sdk/credential-provider-cognito-identity": "3.252.0",
    "@aws-sdk/s3-request-presigner": "3.252.0",
    "@fontsource/inter": "4.5.15",
    "@geoblocks/cesium-view-cube": "0.1.0",
    "@geoblocks/ga-search": "0.0.20",
    "@sentry/browser": "7.31.1",
    "cesium": "1.101.0",
    "d3-array": "3.2.2",
    "d3-axis": "3.0.0",
    "d3-scale": "4.0.2",
    "d3-selection": "3.0.0",
    "d3-shape": "3.2.0",
    "earcut": "2.2.4",
    "event-target-shim": "6.0.2",
    "fomantic-ui-css": "2.9.1",
    "i18next": "22.4.9",
    "i18next-http-backend": "2.1.1",
    "interactjs": "1.10.17",
    "jquery": "3.6.3",
    "jszip": "3.10.1",
    "lit": "2.6.1",
    "loc-i18next": "0.1.4",
    "papaparse": "5.3.2",
    "proj4": "2.8.1",
