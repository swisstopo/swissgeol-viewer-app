{
  "name": "ngm",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint",
    "extract-from-assets": "node scripts/get_assets_properties.js src/assets_properties.js",
    "extract-i18n": "i18next -c i18next-parser.config.cjs",
    "start": "npm run build:static && vite serve",
    "build": "npm run build:clean && npm run build:static && npm run build:js",
    "build:clean": "rimraf dist/",
    "build:static": "npm run build:static:environment && npm run build:static:manuals && npm run build:static:versions",
    "build:static:environment": "node scripts/environment.js",
    "build:static:manuals": "node scripts/generate_manuals.js",
    "build:static:versions": "node scripts/extract_versions.js",
    "build:js": "vite build",
    "check": "npm run check:src && npm run check:tests",
    "check:src": "tsc -p tsconfig.json --noEmit",
    "check:tests": "tsc -p tsconfig.test.json --noEmit",
    "test": "cross-env TS_NODE_PROJECT=tsconfig.test.json node --loader ./node_modules/ts-node/esm.mjs --experimental-specifier-resolution=node node_modules/mocha/bin/mocha -- --exit",
    "e2e": "start-server-and-test start http-get://localhost:8000 cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "./scripts/cypress_run",
    "analyze": "webpack --profile --json > stats.json"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "3.693.0",
    "@aws-sdk/client-s3": "3.693.0",
    "@aws-sdk/credential-provider-cognito-identity": "3.693.0",
    "@aws-sdk/s3-request-presigner": "3.693.0",
    "@fontsource/inter": "5.1.0",
    "@geoblocks/cesium-view-cube": "0.1.4",
    "@geoblocks/ga-search": "0.0.22",
    "@lit/context": "^1.1.3",
    "@lit/task": "^1.0.1",
    "cesium": "1.123.1",
    "d3-array": "3.2.4",
    "d3-axis": "3.0.0",
    "d3-scale": "4.0.2",
    "d3-selection": "3.0.0",
    "d3-shape": "3.2.0",
    "earcut": "3.0.0",
    "event-target-shim": "6.0.2",
    "fomantic-ui-css": "2.9.3",
    "i18next": "^24.0.2",
    "i18next-http-backend": "^3.0.1",
    "interactjs": "1.10.27",
    "jquery": "^3.7.1",
    "jszip": "3.10.1",
    "lit": "3.2.1",
    "loc-i18next": "0.1.6",
    "papaparse": "5.4.1",
    "proj4": "2.14.0",
    "rxjs": "7.8.1",
    "vite": "^6.0.7"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/plugin-proposal-decorators": "7.25.9",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@rollup/plugin-babel": "6.0.4",
    "@types/chai": "5.0.1",
    "@types/geojson": "7946.0.14",
    "@types/jquery": "^3.5.32",
    "@types/mocha": "10.0.9",
    "@types/node": "22.9.0",
    "@typescript-eslint/eslint-plugin": "8.15.0",
    "@typescript-eslint/parser": "8.15.0",
    "autoprefixer": "10.4.20",
    "babel-loader": "9.2.1",
    "chai": "5.1.2",
    "concurrently": "^9.1.0",
    "copy-webpack-plugin": "12.0.2",
    "core-js": "3.39.0",
    "cross-env": "^7.0.3",
    "css-loader": "7.1.2",
    "cypress": "13.15.2",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-cypress": "4.1.0",
    "eslint-plugin-lit": "^1.15.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-sort-class-members": "^1.21.0",
    "file-loader": "6.2.0",
    "file-saver": "2.0.5",
    "globals": "^15.12.0",
    "i18next-parser": "9.0.2",
    "jsdom": "25.0.1",
    "jsdom-global": "3.0.2",
    "marked": "15.0.1",
    "mini-css-extract-plugin": "2.9.2",
    "mocha": "10.8.2",
    "node-fetch": "3.3.2",
    "postcss": "8.4.49",
    "postcss-import": "^16.1.0",
    "postcss-inline-svg": "^6.0.0",
    "postcss-url": "^10.1.3",
    "prettier": "3.4.2",
    "rimraf": "^6.0.1",
    "sortablejs": "1.15.3",
    "source-map-loader": "5.0.0",
    "start-server-and-test": "2.0.8",
    "stream-to-promise": "3.0.0",
    "style-loader": "^4.0.0",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.6.3",
    "vite-plugin-static-copy": "^2.2.0",
    "webpack": "5.96.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.1.0"
  }
}
